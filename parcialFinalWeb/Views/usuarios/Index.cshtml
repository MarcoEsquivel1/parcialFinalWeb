@model IEnumerable<parcialFinalWebEntidades.usuarios>


@{
    ViewBag.Title = "Index";
}

@Html.ActionLink("CrearUsuario", "Crear") 

<table class="table">
    <tr>
        <th>
            user
        </th>
        
        <th>Grados Academicos</th>
        <th>Experiencia</th>
        <th> opciones</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.username
            </td>
            
            <td>
                @Html.ActionLink("Agregar", "CrearGrado", new { id = item.idUsuario}, new { @class = "btn btn-primary" })
            </td>
            <td>
                @Html.ActionLink("ir", "index","experiencias", new { id = item.idUsuario }, new { @class = "btn btn-primary" })
            </td>

            <td>

                @Html.ActionLink("Editar", "Editar", new { id = item.idUsuario }, new { @class = "btn btn-primary" }) |
                @Html.ActionLink("Eliminar", "Eliminar", new { id = item.idUsuario }, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }
</table>

